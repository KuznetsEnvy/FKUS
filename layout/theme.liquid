<body class="template-{{ template | split: '.' | first }} {{ page_title | handle }}">
<!-- Hello world! 2 -->

<div id="header" class="cart" style="width:100%;">
    <div class="container">
        <div class="page-header">
            <div id="toolbar" class="toolbar cf">
                <div class="logo-nav">
                    <div class="logo">
                        {% if settings.use_logo %}{{ 'cart-header-logo-black.png' | asset_url | split:"?" | first | img_tag | link_to: shop.url }}{% else %}{{ shop.name | link_to: shop.url }}{% endif %}
                    </div>

                    {% unless settings.header_layout == 'nav' %}
                    <div class="cf">{{ logo_html }}</div>
                    {% endunless %}

                    {% unless settings.header_layout == 'mobile' %}
                    {% include 'main-navigation' %}
                    {% endunless %}
                </div>


                {% unless settings.header_layout == 'mobile' %}
                <span class="toolbar-links">
                    {% if shop.customer_accounts_enabled %}
                        {% if customer %}
                            <a href="/account">{{ 'layout.customer.your_account' | t }}</a>
                            <span class="divider">/</span> {{ 'layout.customer.logout' | t | customer_logout_link }}
                        {% else %}
                            {{ 'layout.customer.login' | t | customer_login_link }}
                            {% if shop.customer_accounts_optional %}
                                <span class="divider">/</span> {{ 'layout.customer.create_account' | t | customer_register_link }}
                            {% endif %}
                        {% endif %}
                        {% if settings.show_header_search %}
                        <span class="divider">/</span>
                        {% endif %}
                    {% endif %}
                    {% if settings.show_header_search %}
                        <form class="search-form" action="/search" method="get">
                            {% if settings.search_prods_only %}<input type="hidden" name="type" value="product">{% endif %}
                            <button class="icon-mag"></button>
                            <input type="text" name="q" placeholder="{{ 'layout.header.search' | t }}"/>
                        </form>
                    {% endif %}
                  </span>
                {% endunless %}
                <div style="display:none;">
                    <span class="toolbar-cart">
                    <span class="{% unless settings.show_cart_as_button %}no-{% endunless %}btn">
                        <a class="current-cart {% if template != 'cart' %}toggle-cart-summary{% endif %}" href="/cart">
                        {{ cart.item_count }} / {{ cart.total_price | money }} <i class="icon-cart"></i>
                        </a>
                    </span>
                    </span>
                </div>
                <div class="main-menu">
                    <nav class="menu">
                        <ul class="site-nav" id="accessibleNav">
                            {% for link in linklists.main-menu.links %}
                                {% assign child_list_handle = link.title | handleize %}
                                {% if linklists[child_list_handle].links != blank %}
                                    {% comment %}
                                <li class="site-nav--has-dropdown{% if link.active %} site-nav--active{% endif %}" aria-haspopup="true">
                                    <a href="{{ link.url }}">{{ link.title }}</a>
                                    <ul class="site-nav--dropdown">
                                        {% for childlink in linklists[child_list_handle].links %}
                                            <li {% if childlink.active %}class="site-nav--active"{% endif %}>
                                                <a href="{{ childlink.url }}">{{ childlink.title | escape }}</a></li>
                                        {% endfor %}
                                    </ul>
                                    </li>{% endcomment %}
                                {% else %}
                                    {% comment %}
                                    <li {% if link.active %}class="site-nav--active"{% endif %}>
                                        <a href="{{ link.url }}">{{ link.title }}</a>
                                    </li>
                                    {% endcomment %}
                                {% endif %}
                            {% endfor %}
                        </ul>
                        <a class="current-cart {% if template != 'cart' %}toggle-cart-summary{% endif %}"><span class="cart-count">{{ cart.item_count }}</span>items
                            <i class="icon-cart"></i></a>
                    </nav>
                </div>
                {% unless settings.header_layout == 'mobile' %}
                <span class="toolbar-social">{% include 'social-links' %}</span>
                {% endunless %}
            </div>
        </div>
    </div>
    {% include 'cart-summary' %}
</div>

<div id="page-wrap">
    <div id="page-wrap-inner">
        <div id="page-wrap-content">
            <div id="content" data-template="{{ template }}">

                {% if template contains 'index' or template contains 'product' %}
                    {% if settings.home_show_slideshow %}

                        <div id="bicolorBanner" class="banner-index" style="{% if template contains 'product' %}display:none;{% endif %}">
                            <picture>
                                <source media="(min-width: 1920px)" srcset="{{ 'PC-Header-3-1920-sale.jpg' | asset_url | split:"?" | first }}">
                                <source media="(min-width: 1400px)" srcset="{{ 'PC-Header-3-1400-sale.jpg' | asset_url | split:"?" | first }}">
                                <source media="(min-width: 1024px)" srcset="{{ 'PC-Header-3-1024-sale.jpg' | asset_url | split:"?" | first }}">
                                <source media="(min-width: 751px)" srcset="{{ 'PC-Header-3-1024-sale.jpg' | asset_url | split:"?" | first }}">
                                <source media="(min-width: 300px)" srcset="{{ 'Mobile-Header-sale.jpg' | asset_url | split:"?" | first }}">
                                <img src="{{ 'PC-Header-3-1920-sale.jpg' | asset_url | split:"?" | first }}" alt="Homepage banner">
                            </picture>
                        </div>

                        <!-- countdown timer -->
                        {% comment %}
                        [powr-countdown-timer id=9fd31ed4_1472213740]
                        {% endcomment %}

                        {% if settings.shipping-banner-enable %}
                            {% unless template contains 'product' %}
                            <div class="shipping-banner">
                                <div class="nn-secondary-middle">{{ settings.shipping-banner }}</div>
                            </div>
                            {% endunless %}
                        {% endif %}
                    {% endif %}

                {% else %}
                    {% if template contains 'cart' or template contains 'page.support' or template contains 'page.win-cash' or template contains 'page.thank-you' or template contains 'index' %}
                        <div class="cart-header">
                            {% if template contains 'cart' %}
                                {% if settings.cart_banner %}
                                    {% comment %}<img src="{{ 'cart-banner.jpg' | asset_url | split:"?" | first }}" alt="cart-banner"/> {% endcomment %}
                                    <div id="bicolorBanner" class="banner-index" style="{% if template contains 'product' %}display:none;{% endif %}">
                                        <picture>
                                            <source media="(min-width: 64em)" srcset="{{ 'cart-banner.jpg' | asset_url | split:"?" | first }}">
                                            <source media="(min-width: 300px)" srcset="{{ 'Cart-Header-Mobile.jpg' | asset_url | split:"?" | first }}">
                                            <img src="{{ 'cart-banner.jpg' | asset_url | split:"?" | first }}" alt="Sunglasses Original Bicolor">
                                        </picture>
                                    </div>
                                {% endif %}
                            {% elsif template contains 'page.support' %}
                                {% if settings.cart_banner %}
                                    <img src="{{ 'support-banner.jpg' | asset_url | split:"?" | first }}" alt="support-banner"/>
                                {% endif %}
                            {% elsif template contains 'page.win-cash' or template contains 'page.thank-you' %}
                                {% if settings.win_banner %}
                                    <picture>
                                        <source media="(min-width: 768px)" srcset="{{ 'win-cash.jpg' | asset_url | split:"?" | first }}">
                                        <source media="(min-width: 200px)" srcset="{{ 'Mobile-Win-Header.jpg' | asset_url | split:"?" | first }}">
                                        <img src="{{ 'win-cash.jpg' | asset_url | split:"?" | first }}" alt="{{ settings.win-cash-alt }}"/>
                                    </picture>
                                {% endif %}
                            {% endif %}
                            {% else %}
                            <div class="header">
                                {% endif %}

                                {% if template contains 'page.win-cash' %}
                                    <div class="shipping-banner">
                                        <div class="nn-secondary-middle">{{ settings.win-cash-banner }}</div>
                                    </div>
                                {% else %}
                                    {% unless template contains 'page.thank-you' %}
                                        {% if settings.shipping-banner-enable %}
                                            <div class="shipping-banner">
                                                <div class="nn-secondary-middle">{{ settings.shipping-banner }}</div>
                                            </div>
                                        {% endif %}
                                    {% endunless %}
                                {% endif %}
                                <div id="header" style="display:none;"></div>
                            </div> <!--   end header class  -->
                            {% endif %}
                            {{ content_for_layout }}
                        </div><!-- /.cart-header"-->
            </div><!-- /#content -->
        </div><!-- /#page-wrap-content -->
        {% include 'footer' %}
    </div> <!--  page-wrap-inner  -->
</div><!-- /#page-wrap -->

{% include 'mobile-navigation' %}
{% comment %} {% include 'popup' %} {% endcomment %}

{% if template contains 'product' %}
