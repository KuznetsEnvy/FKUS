<body class="template-{{ template | split: '.' | first }} {{ page_title | handle }}">
<!-- Hello world! 2 -->

<div id="header" class="cart" style="width:100%;">
  <div class="page-header">
    <a class="text-logo" href="{{ shop.url }}"><strong>Floatie</strong> Kings</a>
    <div class="top-nav">
      <div id="nav-icon3" class="visible-mobile">
        <span></span>
        <span></span>
        <span></span>
        <span></span>
      </div>
      <nav class="visible-desktop">
        <a href="javascript:void(0)">Shop</a>
        <a href="javascript:void(0)">Our story</a>
        <a href="javascript:void(0)">Join the family</a>
      </nav>
    </div>
    <div class="cart-wrap">
      <a class="current-cart {% if template != 'cart' %}toggle-cart-summary{% endif %}">{{ cart.item_count }}</a>
    </div>
  </div>
  {% include 'cart-summary' %}
</div>

<div id="page-wrap">
  <div id="page-wrap-inner">
    <div id="page-wrap-content">
      <div id="content" data-template="{{ template }}">

        {% if template contains 'index' or template contains 'product' %}

        <!-- Replaced .banner-index from here to index template file -->

        {% else %}
          {% if template contains 'cart' or template contains 'page.support' or template contains 'page.win-cash' or template contains 'page.thank-you' or template contains 'index' %}
            <div class="cart-header">
              {% if template contains 'cart' %}
                {% if settings.cart_banner %}
                  <div id="bicolorBanner" class="banner-index">
                    <picture>
                      <source media="(min-width: 64em)" srcset="{{ 'cart-banner.jpg' | asset_url | split:"?" | first }}">
                      <source media="(min-width: 300px)" srcset="{{ 'Cart-Header-Mobile.jpg' | asset_url | split:"?" | first }}">
                      <img src="{{ 'cart-banner.jpg' | asset_url | split:"?" | first }}" alt="Sunglasses Original Bicolor">
                    </picture>
                  </div>
                {% endif %}
              {% elsif template contains 'page.support' %}
                {% if settings.cart_banner %}
                  <img src="{{ 'support-banner.jpg' | asset_url | split:"?" | first }}" alt="support-banner"/>
                {% endif %}
              {% elsif template contains 'page.win-cash' or template contains 'page.thank-you' %}
                {% if settings.win_banner %}
                  <picture>
                    <source media="(min-width: 768px)" srcset="{{ 'win-cash.jpg' | asset_url | split:"?" | first }}">
                    <source media="(min-width: 200px)" srcset="{{ 'Mobile-Win-Header.jpg' | asset_url | split:"?" | first }}">
                    <img src="{{ 'win-cash.jpg' | asset_url | split:"?" | first }}" alt="{{ settings.win-cash-alt }}"/>
                  </picture>
                {% endif %}
              {% endif %}
          {% else %}
            <div class="header">
          {% endif %}

          {% if template contains 'page.win-cash' %}
            <div class="shipping-banner">
              <div class="nn-secondary-middle">{{ settings.win-cash-banner }}</div>
            </div>
          {% else %}
            {% unless template contains 'page.thank-you' %}
              {% if settings.shipping-banner-enable %}
                <div class="shipping-banner">
                  <div class="nn-secondary-middle">{{ settings.shipping-banner }}</div>
                </div>
              {% endif %}
            {% endunless %}
          {% endif %}
          <div id="header" style="display:none;"></div>
        </div><!-- /.header -->
        {% endif %}
        {{ content_for_layout }}
      </div><!-- /.cart-header -->
      </div><!-- /#content -->
    </div><!-- /#page-wrap-content -->
    {% include 'footer' %}
  </div> <!--  page-wrap-inner  -->
</div><!-- /#page-wrap -->


