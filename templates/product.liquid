{% assign variant = product.selected_or_first_available_variant %}
{% assign featured_image = product.selected_or_first_available_variant.featured_image | default: product.featured_image %}

<div itemscope itemtype="http://schema.org/Product">
  <meta itemprop="url" content="{{ shop.url }}{{ product.url }}" />
  <meta itemprop="image" content="{{ product.featured_image | product_img_url: 'grande' }}" />
  {% for variant in product.variants %}
    <div class="container" itemprop="offers" itemscope itemtype="http://schema.org/Offer">
      <meta itemprop="price" content="{{ variant.price | money_without_currency | strip_html }}" />
      <meta itemprop="priceCurrency" content="{{ shop.currency }}" />
      {% if product.available %}
      <link itemprop="availability" href="http://schema.org/InStock" />
      {% else %}
      <link itemprop="availability" href="http://schema.org/OutOfStock" />
      {% endif %}
    </div>
  {% endfor %}

  <div class="inline-row"></div>

  <div class="space-under">
    <div class="product-top-wrap container">

      <div class="product-gallery">
        <div class="main">
          {% if product.images.size > 1 %}
            <ul class="bxslider">
              {% for image in product.images %}
                <li><img class="main-img" src="{{ image | product_img_url: '1024x1024' }}" alt="{{ image.alt | escape }}" /></li>
              {% endfor %}
            </ul>
            <div class="bx-pager">
              {% for image in product.images %}
                <a href="" data-slide-index="{{ forloop.index0 }}"><img src="{{ image | product_img_url: '180x180' }}" alt="" /></a>
              {% endfor %}
            </div>
          {% else %}
            <ul>
              <li><img class="main-img" src="{{ featured_image | product_img_url: '1024x1024' }}" alt="{{ featured_image.alt | escape }}" /></li>
            </ul>
          {% endif %}
        </div>
      </div>

      <form id="product-form" action="/cart/add" method="post" enctype="multipart/form-data" data-product-id="{{ product.id }}">
        <div class="border-bottom section padless-top product-title-row">
          <div class="product-title">
            <h1 class="title" itemprop="name">{{ product.title }}</h1>
          </div>
          <div class="product-price">
            <div id="price" class="product-price {% if product.variants.size == 1 and product.compare_at_price_max > product.price %}on-sale{% endif %}">
              {% if product.variants.size == 1 and product.compare_at_price_max > product.price %}<del>{{ product.compare_at_price_max | money }}</del>{% endif %}
              {{ product.price | money }}
            </div>
          </div>
          <div class='jdgm-widget jdgm-preview-badge' data-id='{{ product.id }}'>{{ product.metafields.judgeme.badge }}</div>
          <div class="add-ons-section visible-desktop">
            <div class="header">Add these:</div>
            <div class="add-ons-wrap">
              {% assign counter = 0 %}

              {% unless product.handle contains 'pump' %}
                {% assign addon = all_products["super-pump-2000"] %}
                {% if addon.available %}
                  {% assign counter = counter | plus: 1 %}
                  <div class="add-ons-item" data-variant-id="{{ addon.variants.first.id }}">
                    <div class="image"><img src="{{ addon | img_url: '150x' }}" alt="" /></div>
                    <div class="desc">
                      <div class="title">{{ addon.title }}</div>
                      <div class="checkbox">
                        <div class="price">+ {{ addon.price | money }}</div>
                        <div class="bttn blue"><span></span><span></span></div>
                        <div class="bttn green hidden">✓</div>
                      </div>
                    </div>
                  </div>
                {% endif %}
              {% endunless %}

              {% for tag in product.tags %}
                {% if counter < 4 %}
                  {% for addon in collections['all'].products %}
                    {% if addon.tags contains tag and counter < 4 and product.id != addon.id and addon.available %}
                      {% assign counter = counter | plus: 1 %}
                      <div class="add-ons-item" data-variant-id="{{ addon.variants.first.id }}">
                        <div class="image"><img src="{{ addon | img_url: '150x' }}" alt="" /></div>
                        <div class="desc">
                          <div class="title">{{ addon.title }}</div>
                          <div class="checkbox">
                            <div class="price">+ {{ addon.price | money }}</div>
                            <div class="bttn blue"><span></span><span></span></div>
                            <div class="bttn green hidden">✓</div>
                          </div>
                        </div>
                      </div>
                    {% endif %}
                  {% endfor %}
                {% endif %}
              {% endfor %}

              {% if counter < 4 %}
                {% assign max = 4 | minus: counter %}
                <div class="debug max">{{ max }}</div>
                {% for addon in collections['bestsellers'].products limit: max %}
                  {% if addon.available %}
                    {% assign counter = counter | plus: 1 %}
                    <div class="add-ons-item" data-variant-id="{{ addon.variants.first.id }}">
                      <div class="image"><img src="{{ addon | img_url: '150x' }}" alt="" /></div>
                      <div class="desc">
                        <div class="title">{{ addon.title }}</div>
                        <div class="checkbox">
                          <div class="price">+ {{ addon.price | money }}</div>
                          <div class="bttn blue"><span></span><span></span></div>
                          <div class="bttn green hidden">✓</div>
                        </div>
                      </div>
                    </div>
                  {% endif %}
                {% endfor %}
              {% endif %}
            </div>
          </div>
        </div>
        <div class="section">
          {% if settings.prod_show_sku and product.variants.first.sku != blank %}
            <div class="space-under sku-row">{{ 'products.product.sku' | t }} <span id="sku">{{ product.variants.first.sku }}</span></div>
          {% endif %}

          {% if product.available %}
            {% assign hide_default_title = false %}
            {% if product.variants.size == 1 %}
              {% if product.variants.first.title contains 'Default' %}
                {% assign hide_default_title = true %}
              {% endif %}
              {% unless settings.prod_singlevar_mode contains 'drop' %}
                {% assign hide_default_title = true %}
              {% endunless %}
            {% endif %}

            {% unless product.variants.size != 1 or product.variants.first.title contains 'Default' %}
              {% if settings.prod_singlevar_mode contains 'text' %}
                <div class="variant-info">
                  {% for option in product.options %}
                    {% assign option_key = 'option' | append: forloop.index %}
                    <span class="option">{% if settings.prod_singlevar_mode contains 'withlabel' %}<span class="label">{{ option }}:</span>{% endif %} {{ product.variants.first[option_key] }}</span>{% unless forloop.last %},{% endunless %}
                  {% endfor %}
                </div>
              {% endif %}
            {% endunless %}


            <div id="product-variants" class="{% if hide_default_title %} hidden{% endif %}">
              <select id="product-select" name="id" class="hidden">
                {% for variant in product.variants %}
                  <option {% if variant == product.selected_or_first_available_variant %} selected="selected" {% endif %} value="{{ variant.id }}">{{ variant.title }} - {{ variant.price | money }}</option>
                {% endfor %}
              </select>
            </div>



            <div class="js-btn-daddy">
              <button name="add" class="add-to-cart buy-now" id="add" data-variant-id="{{ product.variants.first.id }}">{{ 'products.product.add_to_cart_html' | t }}</button>
            </div>

            <div id="backorder" class="hidden" style="padding-top:50px;">
              <p><span id="selected-variant"></span> {{ 'products.product.backorder' | t }}</p>
            </div>
          {% else %}

            <div class="rte">
              <a href="javascript:void(0)" style="text-align:center" class="back-in-stock add-to-cart notify button button--black-gradient cart-btn cart-btn-desktop unavailable"> {{'products.product.product_unavailable_html' | t }}</a>
            </div>
          {% endif %}
        </div>
        <div class="banner-high-quality visible-desktop">
          <svg id="icon-high-quality" data-name="High quality" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 29.06 35.03">
            <title>icon-high-quality</title>
