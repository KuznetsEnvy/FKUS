          <div class="product-bottom-wrap">
            <div class="border-bottom section padless-top product-title-row">
              <div class="product-title">
                <h3 class="title" itemprop="name">{{ product.title }}</h3>
                <div id="price" class="product-price {% if product.variants.size == 1 and product.compare_at_price_max > product.price %}on-sale{% endif %}">
                  {% if product.variants.size == 1 and product.compare_at_price_max > product.price %}<del>{{ product.compare_at_price_max | money }}</del>{% endif %}
                  {{ product.price | money }}
                </div>
              </div>
              <div class='jdgm-widget jdgm-preview-badge' data-id='{{ product.id }}'>{{ product.metafields.judgeme.badge }}</div>
            </div>
            <div class="mobile1 section {% unless settings.prod_desc_loc == 'under' %}border-bottom{% endunless %}">
              {% if settings.prod_show_sku and product.variants.first.sku != blank %}
              <div class="space-under sku-row">{{ 'products.product.sku' | t }} <span id="sku">{{ product.variants.first.sku }}</span></div>
              {% endif %}

              {% if product.available %}

                {% assign hide_default_title = false %}
                {% if product.variants.size == 1 %}

                  {% if product.variants.first.title contains 'Default' %}
                    {% assign hide_default_title = true %}
                  {% endif %}

                  {% unless settings.prod_singlevar_mode contains 'drop' %}
                    {% assign hide_default_title = true %}
                  {% endunless %}
                {% endif %}

                {% unless product.variants.size != 1 or product.variants.first.title contains 'Default' %}
                  {% if settings.prod_singlevar_mode contains 'text' %}
                    <div class="variant-info">
                      {% for option in product.options %}
                        {% assign option_key = 'option' | append: forloop.index %}
                        <span class="option">{% if settings.prod_singlevar_mode contains 'withlabel' %}<span class="label">{{ option }}:</span>{% endif %} {{ product.variants.first[option_key] }}</span>{% unless forloop.last %},{% endunless %}
                      {% endfor %}
                    </div>
                  {% endif %}
                {% endunless %}


                <div id="product-variants" class="{% if hide_default_title %} hidden{% endif %}">
                  <select id="product-select" name="id" class="hidden">
                    {% for variant in product.variants %}
                    <option {% if variant == product.selected_or_first_available_variant %} selected="selected" {% endif %} value="{{ variant.id }}">{{ variant.title }} - {{ variant.price | money }}</option>
                    {% endfor %}
                  </select>
                </div>



                <div class="js-btn-daddy">
                  <button name="add" class="add-to-cart buy-now" id="add" data-variant-id="{{ product.variants.first.id }}">{{ 'products.product.add_to_cart_html' | t }}</button>
                </div>

                <div id="backorder" class="hidden" style="padding-top:50px;">
                  <p><span id="selected-variant"></span> {{ 'products.product.backorder' | t }}</p>
                </div>

              {% else %}

                <div class="rte">
                  <a href="javascript:void(0)" style="text-align:center" class="back-in-stock add-to-cart notify button button--black-gradient cart-btn cart-btn-desktop unavailable"> {{'products.product.product_unavailable_html' | t }}</a>
                </div>

              {% endif %}

            </div>
          </div>

          {% assign gallery = product.metafields.gallery %}
          {% assign s = gallery.size %}
          {% assign hasphotos = "false" %}
          {% for image in product.metafields.gallery %}
            {% if image.last != "blank" %}
              {% assign hasphotos = "true" %}
            {% endif %}
          {% endfor %}
          {% if hasphotos == "true" %}
            <div id="insta" class="prod-galary">

                <h2 class="section-header line-framed"><span>As seen on </span><em>Instagram</em></h2>
                <div class="grid product">
                  {% for image in product.metafields.gallery %}
                    {% if image != blank and image.last != blank and image.last != 'blank' %}
                      <div class="grid-item"><img src="{{ image.last }}" alt="{{ product.title | escape }}" /></div>
                    {% endif %}
                  {% endfor %}
                </div>

            </div>
          {% endif %}
        </form>

        <div id='judgeme_product_reviews' class='jdgm-widget jdgm-review-widget' data-product-title='{{ product.title | escape }}' data-id='{{ product.id }}'>{{ product.metafields.judgeme.widget }}</div>
      </div>
    </div><!-- /.product-details -->
  </div>
</div>

